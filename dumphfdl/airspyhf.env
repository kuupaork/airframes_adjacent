DUMPHFDL_CORE_IDS='1,2,3,4'
DUMPHFDL_FRINGE_IDS='5,6,7,8'

# enter any ranges that you specifically want to ignore.
DUMPHFDL_IGNORE_RANGES=0-6000

DUMPHFDL_WATCH_INTERVAL=600
DUMPHFDL_SDR_SETTLE=2
DUMPHFDL_SYSTABLE="<path to dumphfdl's>/systable.conf"

DUMPHFDL_STATION_ID="MY STATION ID HERE"

DUMPHFDL_AIRFRAMES_CACHE=hfdl.json
# this is temporary until Airframes's own is back online.

DUMPHFDL_BACKUP_URL='https://ork.rodeo/volatile/hfdl-ground-stations.json'
